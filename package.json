{
  "name": "webmangler-monorepo",
  "description": "A mangler for websites",
  "license": "MIT",
  "version": "0.1.0-alpha",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ericcornelissen/webmangler"
  },
  "author": {
    "name": "Eric Cornelissen",
    "email": "ericornelissen@gmail.com",
    "url": "https://ericcornelissen.dev/"
  },
  "scripts": {
    "benchmark": "TEST_ENV=benchmark mocha",
    "benchmark:core": "TEST_PKG=core npm run benchmark",
    "clean": "node scripts/clean.js",
    "coverage": "nyc mocha",
    "coverage:benchmarking": "TEST_PKG=benchmarking npm run coverage",
    "coverage:cli": "TEST_PKG=cli npm run coverage",
    "coverage:core": "TEST_PKG=core npm run coverage",
    "coverage:testing": "TEST_PKG=testing npm run coverage",
    "format": "npm run lint -- --fix",
    "lint": "eslint . --ext .js,.json,.md,.ts,.yml",
    "postinstall": "husky install scripts/hooks",
    "test": "mocha",
    "test:benchmarking": "TEST_PKG=benchmarking npm run test",
    "test:cli": "TEST_PKG=cli npm run test",
    "test:core": "TEST_PKG=core npm run test",
    "test:testing": "TEST_PKG=testing npm run test",
    "test:watch": "mocha --watch --reporter min"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "12.1.1",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/chai": "4.2.16",
    "@types/mocha": "8.2.2",
    "@types/node": "14.14.41",
    "@types/ramda": "0.27.40",
    "@types/sinon": "10.0.0",
    "@types/sinon-chai": "3.2.5",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "chai": "4.3.4",
    "commitlint": "12.1.1",
    "eslint": "7.24.0",
    "eslint-plugin-chai-expect": "2.2.0",
    "eslint-plugin-jsdoc": "32.3.0",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-md": "1.0.19",
    "eslint-plugin-mocha": "8.1.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-yml": "0.9.0",
    "husky": "6.0.0",
    "mocha": "8.3.2",
    "nyc": "15.1.0",
    "ramda": "0.27.1",
    "remark-lint-final-newline": "1.0.5",
    "remark-lint-first-heading-level": "2.0.1",
    "remark-lint-no-duplicate-defined-urls": "1.0.1",
    "remark-lint-no-duplicate-definitions": "2.0.1",
    "remark-lint-no-duplicate-headings-in-section": "2.0.2",
    "remark-lint-no-empty-url": "2.0.1",
    "remark-lint-no-heading-content-indent": "3.0.0",
    "remark-lint-no-heading-indent": "3.0.0",
    "remark-lint-no-heading-like-paragraph": "2.0.1",
    "remark-lint-no-html": "2.0.1",
    "remark-lint-no-paragraph-content-indent": "3.0.0",
    "remark-lint-no-reference-like-url": "2.0.1",
    "sinon": "10.0.0",
    "sinon-chai": "3.6.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "engines": {
    "node": ">=12.16.0"
  }
}
